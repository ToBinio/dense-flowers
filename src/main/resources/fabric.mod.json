{
	"schemaVersion": 1,
	"id": "dense-flowers",
	"version": "${version}",
	"name": "Dense Flowers",
	"description": "Render Flowers more dense",
	"authors": [
		"ToBinio"
	],
	"contact": {
		"repo": "https://github.com/toBinio/dense-flowers",
		"homepage": "https://github.com/toBinio/dense-flowers",
		"issues": "https://github.com/toBinio/dense-flowers/issues",
		"sources": "https://github.com/toBinio/dense-flowers"
	},
	"license": "MIT",
	"icon": "assets/dense-flowers/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"tobinio.denseflowers.DenseFlowers"
		],
		"client": [
			"tobinio.denseflowers.DenseFlowersClient"
		]
	},
	"mixins": [
		"dense-flowers.mixins.json",
		{
			"config": "dense-flowers.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}